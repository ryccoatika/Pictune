<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                    android:width="15dp"
                    android:color="@android:color/white" />
            <size
                    android:width="30dp"
                    android:height="30dp" />
        </shape>
    </item>
    <item
            android:left="1dp"
            android:top="1dp"
            android:right="1dp"
            android:bottom="1dp">
        <shape android:shape="oval">
            <stroke
                    android:width="5dp"
                    android:color="@android:color/black" />
            <size
                    android:width="30dp"
                    android:height="30dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                    android:width="20dp"
                    android:color="@android:color/transparent" />
            <solid android:color="@android:color/black" />
            <size
                    android:width="30dp"
                    android:height="30dp" />
        </shape>
    </item>
</layer-list>
